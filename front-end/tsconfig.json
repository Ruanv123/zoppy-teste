{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "paths": {
      "@spartan-ng/helm/button": [
        ".\\src/app/shared/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/helm/dialog": [
        ".\\src/app/shared/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        ".\\src/app/shared/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/helm/label": [
        ".\\src/app/shared/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/helm/sonner": [
        ".\\src/app/shared/ui-sonner-helm/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        ".\\src/app/shared/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/helm/pagination": [
        ".\\src/app/shared/ui-pagination-helm/src/index.ts"
      ],
      "@spartan-ng/helm/select": [
        ".\\src/app/shared/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/helm/table": [
        ".\\src/app/shared/ui-table-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
